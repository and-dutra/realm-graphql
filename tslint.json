{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "no-submodule-imports": [
      true,
      "apollo-client"
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": {
      "multiline": "never",
      "singleline": "never"
    },
    "interface-name": [
      true,
      "never-prefix"
    ]
  }
}
